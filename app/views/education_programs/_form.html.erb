<%= simple_form_for(@education_program) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :course %>

    <%= f.input :educationLevel %>

    <%= f.input :educationForm %>

    <%= f.input :name %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
